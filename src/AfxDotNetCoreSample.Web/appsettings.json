{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServerUrls": "http://*:80",
  "Threads": {
    "Min": 0,
    "IO": 0
  },
  "Encrypt": {
    "DesKey": "$Afx@cn#"
  },
  "Database": {
    "Type": "MSSQLServer",
    "Init": true,
    "IsLog": true,
    "MSSQLServer": "Data Source=127.0.0.1,1433;Initial Catalog=AfxDotNetCoreSample;User ID=sa;Password=123456",
    "MySQL": "Server=127.0.0.1;Port=3306;Database=AfxDotNetCoreSample;User Id=root;Password=123456;CharacterSet=utf8"
  },
  "Cache": {
    "Type": "Redis",
    "Prefix": "AfxDotNetCoreSample:"
  },
  "RedisConfig": "127.0.0.1:6379,syncTimeout=3000,name=AfxDotNetCoreSample",
  "IdGenerator": {
    "ServerId": 1000,
    "CacheCount": 10
  },
  "LogSaveDay": 7
}
