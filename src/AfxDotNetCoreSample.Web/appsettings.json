{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AfxDotNetCoreSample": {
    "ServerUrls": "http://*:5000",
    "Encrypt": {
      "DesKey": "$Afx@cn#"
    },
    "Database": {
      "Type": "MSSQLServer",
      "Init": true,
      "IsLog": true,
      "MSSQLServer": "Data Source=127.0.0.1,1433;Initial Catalog=AfxDotNetCoreSample;User ID=sa;Password=123456;Application Name=Afx",
      "MySQL": "Server=loaclhost;Port=3306;Database=AfxDotNetCoreSample;User Id=root;Password=123@abc;CharacterSet=utf8"
    },
    "Cache": {
      "Type": "Redis",
      "Prefix": "AfxDotNetCoreSample:"
    },
    "RedisConfig": "127.0.0.1:6379",
    "IdGenerator": {
      "ServerId": 1000,
      "CacheCount": 10
    },
    "Ioc": {
      "Service": "AfxDotNetCoreSample.Service",
      "Repository": "AfxDotNetCoreSample.Repository",
      "Cache": "AfxDotNetCoreSample.Cache"
    },
    "LogSaveDay": 7
  }
}
