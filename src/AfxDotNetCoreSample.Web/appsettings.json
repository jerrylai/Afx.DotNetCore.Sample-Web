{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AfxDotNetCoreSample": {
    "ServerUrls": "http://*:80",
    "Threads": {
      "Min": 0,
      "IO": 0
    },
    "Encrypt": {
      "DesKey": "$Afx@cn#"
    },
    "Database": {
      "Type": "MySQL",
      "Init": true,
      "IsLog": true,
      "MSSQLServer": "Data Source=192.168.1.2,1433;Initial Catalog=AfxDotNetCoreSample;User ID=sa;Password=123456",
      "MySQL": "Server=192.168.1.2;Port=3306;Database=AfxDotNetCoreSample;User Id=root;Password=123456;CharacterSet=utf8"
    },
    "Cache": {
      "Type": "Redis",
      "Prefix": "AfxDotNetCoreSample:"
    },
    "RedisConfig": "192.168.1.2:6379,syncTimeout=3000,name=AfxDotNetCoreSample",
    "IdGenerator": {
      "ServerId": 1000,
      "CacheCount": 10
    },
    "Ioc": {
      "Service": "AfxDotNetCoreSample.Service",
      "Repository": "AfxDotNetCoreSample.Repository",
      "Cache": "AfxDotNetCoreSample.Cache"
    },
    "LogSaveDay": 7
  }
}
